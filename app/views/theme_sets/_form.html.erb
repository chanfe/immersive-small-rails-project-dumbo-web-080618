<%= @title %></h1>
<%= form_for @theme_set do |f| %>
  <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
  <%= f.label "Name: ", class: "mdl-textfield__label" %>
  <%= f.text_field :name, class: "mdl-textfield__input" %><br>
  </div>

  <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
     <%= f.label "Category: ", class: "mdl-textfield__label"%>
    <%= f.text_field :category , class: "mdl-textfield__input"%>
  </div>
 
 
  <button type="submit" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent">
  <%= @title.upcase %>
  </button>
<% end %>


