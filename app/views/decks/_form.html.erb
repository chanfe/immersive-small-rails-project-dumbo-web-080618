<%= form_for @deck do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.collection_select :user_id, User.all, :id, :username %>
  <br>

  <!-- <% f.collection_check_boxes :card_ids, Card.all, :id, :name %> -->
  <% Card.all.each do |card| %>
    <%= label_tag "#{card.name}" %>
    <%= number_field_tag "card_ids[#{card.id}]", nil, in: 0..10 %>
    <br>
  <% end %>
 <%= f.submit %>
<% end %>
